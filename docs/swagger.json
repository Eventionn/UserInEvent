{
  "openapi": "3.0.0",
  "info": {
    "title": "API UserInEvent",
    "version": "1.0.0",
    "description": "API para gerir utilizadores em eventos."
  },
  "servers": [
    {
      "url": "http://localhost:5001",
      "description": "Local server"
    }
  ],
  "paths": {
    "/userinevents": {
      "get": {
        "summary": "Obter todos os eventos de utilizadores",
        "operationId": "getAllUserInEvents",
        "tags": ["UserInEvent"],
        "responses": {
          "200": {
            "description": "Lista de eventos de utilizadores",
            "content": {
              "application/json": {
                "example": [
                  {
                    "ticketID": "1a2b3c4d-5678-90ab-cdef-1234567890ab",
                    "event_id": "9876543210",
                    "user_id": "12345",
                    "feedback_id": null,
                    "createdAt": "2024-11-18T12:00:00Z",
                    "participated": true
                  }
                ]
              }
            }
          },
          "500": {
            "description": "Erro interno ao obter os eventos de utilizadores"
          }
        }
      },
      "post": {
        "summary": "Criar um novo evento de utilizador",
        "operationId": "createUserInEvent",
        "tags": ["UserInEvent"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "event_id": "9876543210",
                "user_id": "12345",
                "feedback_id": null,
                "participated": true
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Evento de utilizador criado com sucesso",
            "content": {
              "application/json": {
                "example": {
                  "ticketID": "1a2b3c4d-5678-90ab-cdef-1234567890ab",
                  "event_id": "9876543210",
                  "user_id": "12345",
                  "feedback_id": null,
                  "createdAt": "2024-11-18T12:00:00Z",
                  "participated": true
                }
              }
            }
          },
          "500": {
            "description": "Erro interno ao criar evento de utilizador"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UserInEvent": {
        "type": "object",
        "properties": {
          "ticketID": {
            "type": "string",
            "example": "1a2b3c4d-5678-90ab-cdef-1234567890ab"
          },
          "event_id": {
            "type": "string",
            "example": "9876543210"
          },
          "user_id": {
            "type": "string",
            "example": "12345"
          },
          "feedback_id": {
            "type": "string",
            "example": null
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "example": "2024-11-18T12:00:00Z"
          },
          "participated": {
            "type": "boolean",
            "example": true
          }
        }
      },
      "Feedback": {
        "type": "object",
        "properties": {
          "feedbackID": {
            "type": "string",
            "example": "1a2b3c4d-5678-90ab-cdef-1234567890ab"
          },
          "rating": {
            "type": "integer",
            "example": 5
          },
          "commentary": {
            "type": "string",
            "maxLength": 150,
            "example": "Evento muito bem organizado!"
          }
        }
      }
    }
  }
}
